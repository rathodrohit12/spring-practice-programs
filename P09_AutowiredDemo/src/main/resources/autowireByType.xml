<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/util https://www.springframework.org/schema/util/spring-util.xsd">

    <bean class="com.rohit.bean.Employee" id="emp" autowire="byType">
        <property name="name" value="Ram"></property>
        <property name="age" value="25"></property>
    </bean>

    <bean class="com.rohit.bean.Address" id="adr">
        <property name="city" value="pune"></property>
        <property name="state" value="MH"></property>
        <property name="street" value="abc-road"></property>
    </bean>

    <bean class="com.rohit.bean.Address" id="adr1" primary="true">
        <property name="city" value="Ayodhya"></property>
        <property name="state" value="UP"></property>
        <property name="street" value="Ram-nagri"></property>
    </bean>



</beans>
